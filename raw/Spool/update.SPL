SQL> SELECT DISTINCT B.BRANCH,
  2  		     B.ACC_NO,
  3  		     A.CUST_NO,
  4  		     C.CUSTOMER_NAME1,
  5  		     D.ACC_MSG,
  6  		     B.CIF_SIG_ID,
  7  		     B.SOLO_SUFFICIENT
  8    FROM STTM_CUST_ACCOUNT	A,
  9  	    SVTM_ACC_SIG_DET	B,
 10  	    STTM_CUSTOMER	C,
 11  	    SVTM_ACC_SIG_MASTER D
 12   WHERE A.CUST_AC_NO = B.ACC_NO
 13  	AND A.CUST_NO = C.CUSTOMER_NO
 14  	AND B.ACC_NO = '0221640104102103'
 15  	AND D.ACC_NO = B.ACC_NO
 16  	AND D.BRANCH = A.BRANCH_CODE
 17  	AND A.AUTH_STAT = 'A'
 18  	AND A.RECORD_STAT = 'O'
 19  	AND A.BRANCH_CODE = '022'
 20   ORDER BY B.BRANCH ;

