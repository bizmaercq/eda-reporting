WHERE COND_KEY LIKE '%&ACCOUNT%'
/
PROMPT BACK VALUED EVENTS
SELECT * FROM XAFNFC.ICTBS_BACK_DATED_EVENTS
WHERE ACC LIKE '&ACCOUNT'
/
PROMPT ICTBS_CONSOL_BD_BAL
SELECT C.* FROM XAFNFC.ICTBS_CONSOL_BD_BAL C
WHERE EXISTS ( SELECT 1 FROM XAFNFC.ICTMS_ACC M WHERE M.BRN = C.BRN AND M.CALC_ACC = C.ACC AND M.ACC = '&ACCOUNT')
/
PROMPT XAFNFC.ICTBS_CONSOL_VD_BAL
SELECT C.* FROM XAFNFC.ICTBS_CONSOL_VD_BAL C
WHERE EXISTS ( SELECT 1 FROM XAFNFC.ICTMS_ACC M WHERE M.BRN = C.BRN AND M.CALC_ACC = C.ACC AND M.ACC = '&ACCOUNT')
/
PROMPT ICTB_RACR
SELECT * FROM XAFNFC.ICTBS_RACR WHERE BRN = '&BRANCH' AND ACC = '&ACCOUNT'
/
PROMPT ICTMS_PR_CHG
SELECT * FROM XAFNFC.ICTMS_PR_CHG WHERE PRODUCT_CODE  IN (SELECT DISTINCT PROD FROM XAFNFC.ICVWS_ACC_PR WHERE BRN ='&BRANCH' AND ACC = '&ACCOUNT')
/
PROMPT ICTMS_PRODUCT_DEFINITION
SELECT * FROM XAFNFC.ICTMS_PRODUCT_DEFINITION WHERE PRODUCT_CODE IN (SELECT DISTINCT PROD FROM XAFNFC.ICVWS_ACC_PR WHERE BRN ='&BRANCH' AND ACC = '&ACCOUNT')
