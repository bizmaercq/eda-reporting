ACCEPT ACCOUNT PROMPT 'ENTER THE ACCOUNT >'
ACCEPT BRANCH  PROMPT 'ENTER THE BRANCH  >'
SET ARRAY 1
SET HEAD ON
SET FEEDBACK ON
SET LINE 10000
SET PAGESIZE 10000
SET ECHO ON
SET COLSEP ";"
SET TRIMSPOOL ON
SET NUMFORMAT 99999999999999999999.9999
SPOOL IC_DETAILS.SPL
SELECT * FROM XAFNFC.STTM_DATES WHERE BRANCH_CODE='&BRANCH';
PROMPT ICTMS_PRODUCT_DEFINITION
SELECT * FROM XAFNFC.ICTMS_PRODUCT_DEFINITION
WHERE PRODUCT_CODE IN (SELECT DISTINCT PROD FROM XAFNFC.ICVWS_ACC_PR WHERE BRN ='&BRANCH' AND ACC = '&ACCOUNT')
ORDER BY PRODUCT_CODE
/
PROMPT ICTBS_ACC_PR
SELECT * FROM XAFNFC.ICTBS_ACC_PR WHERE ACC = '&ACCOUNT'
/
PROMPT STTM_CUST_ACCOUNT_DORMANCY
SELECT * FROM XAFNFC.STTM_CUST_ACCOUNT_DORMANCY WHERE cust_ac_no='&ACCOUNT'
/
PROMPT ACCPR HISTORY
SELECT * FROM XAFNFC.ICTB_ACC_PR_HISTORY  WHERE ACC = '&ACCOUNT'
/
PROMPT ICVWS_ACC_PR
SELECT * FROM XAFNFC.ICVWS_ACC_PR WHERE ACC = '&ACCOUNT'
/
PROMPT ACC_ACTION
SELECT * FROM XAFNFC.ICTBS_ACC_ACTION WHERE ACC = '&ACCOUNT'
/

PROMPT ICTBS_MAINT_QUEUE
SELECT * FROM XAFNFC.ICTBS_MAINT_QUEUE
WHERE MAINT_KEY LIKE '%&ACCOUNT%'
/
PROMPT ACTION LOG
SELECT * FROM XAFNFC.ICTBS_ACTION_LOG
WHERE KEY_VAL LIKE '%&ACCOUNT%'
/
PROMPT ICVW_LINE_DET
SELECT * FROM XAFNFC.ICVW_LINE_DET WHERE BRN ='&BRANCH' AND ACC = '&ACCOUNT';
PROMPT ENTRIES
SELECT * FROM XAFNFC.ICTBS_ENTRIES WHERE ACC = '&ACCOUNT'
/
PROMPT ENTRIES HISTORY
SELECT * FROM XAFNFC.ICTBS_ENTRIES_HISTORY WHERE ACC = '&ACCOUNT' ORDER BY ENT_DT
/
PROMPT VD_BAL
SELECT * FROM XAFNFC.ACTBS_VD_BAL WHERE ACC = '&ACCOUNT'
ORDER BY VAL_DT
/
PROMPT IS_VALS
SELECT * FROM XAFNFC.ICTBS_IS_VALS WHERE ACC = '&ACCOUNT'
/
PROMPT ACVW_ALL_AC_ENTRIES_IC
SELECT * FROM XAFNFC.ACVWS_ALL_AC_ENTRIES WHERE AC_BRANCH='&BRANCH'
AND ( AC_NO = '&ACCOUNT' OR RELATED_ACCOUNT = '&ACCOUNT')
AND MODULE='IC'
ORDER BY TRN_DT DESC
/
PROMPT ACVW_ALL_AC_ENTRIES
SELECT * FROM XAFNFC.ACVWS_ALL_AC_ENTRIES WHERE AC_BRANCH='&BRANCH'
AND ( AC_NO = '&ACCOUNT' OR RELATED_ACCOUNT = '&ACCOUNT')
ORDER BY TRN_DT DESC
/
PROMPT STTM_RD_PAYMENTS_DET
SELECT * FROM XAFNFC.STTM_RD_PAYMENTS_DET WHERE BRANCH_CODE='&BRANCH' AND RD_AC_NO='&ACCOUNT'
ORDER BY TXN_DATE
/
PROMPT UDEVALS
SELECT * FROM XAFNFC.ICTBS_UDEVALS WHERE COND_KEY LIKE '%'||(SELECT ACCOUNT_CLASS||CCY FROM XAFNFC.STTM_CUST_ACCOUNT
                                                 WHERE CUST_AC_NO LIKE  '&ACCOUNT') ||'%'
/
PROMPT ICTBS_UDEVALS
SELECT * FROM XAFNFC.ICTBS_UDEVALS
WHERE COND_KEY LIKE '%&ACCOUNT%' ORDER BY PROD  ,UDE_EFF_DT
/
PROMPT ICTBS_UDEVALS_HIST
SELECT * FROM XAFNFC.ICTBS_UDEVALS_HISTORY
WHERE COND_KEY LIKE '%&ACCOUNT%' ORDER BY PROD,UDE_EFF_DT
/
