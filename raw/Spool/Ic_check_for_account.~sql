/
PROMPT CALC QUEUE
SELECT * FROM XAFNFC.ICTBS_CALC_QUEUE
WHERE BRN ='&BRANCH'
AND ACC = '&ACCOUNT'
AND PROD IN (SELECT DISTINCT PROD FROM XAFNFC.ICVWS_ACC_PR WHERE BRN ='&BRANCH' AND ACC = '&ACCOUNT')
ORDER BY PROD
/
PROMPT ICTBS_ACQR
