WHERE COND_KEY LIKE '%'||(SELECT ACCOUNT_CLASS||CCY FROM XAFNFC.STTM_CUST_ACCOUNT
                                                 WHERE CUST_AC_NO LIKE  '&ACCOUNT') ||'%'
