PROMPT 'ALL RULES LINKED TO THE ACCOUNT'
SELECT * FROM XAFNFC.ICTMS_RULE WHERE RULE_ID IN (SELECT RULE FROM XAFNFC.ICTMS_PR_INT
WHERE PRODUCT_CODE IN (SELECT DISTINCT PROD FROM XAFNFC.ICVWS_ACC_PR WHERE BRN ='&BRANCH' AND ACC = '&ACCOUNT'))
/
SELECT * FROM XAFNFC.ICTMS_RULE_FRM WHERE RULE_ID IN (SELECT RULE FROM XAFNFC.ICTMS_PR_INT
WHERE PRODUCT_CODE IN (SELECT DISTINCT PROD FROM XAFNFC.ICVWS_ACC_PR WHERE BRN ='&BRANCH' AND ACC = '&ACCOUNT'))
/
SELECT * FROM XAFNFC.ICTMS_RULE_FRM_ELEMENTS WHERE RULE_ID IN (SELECT RULE FROM XAFNFC.ICTMS_PR_INT
WHERE PRODUCT_CODE IN (SELECT DISTINCT PROD FROM XAFNFC.ICVWS_ACC_PR WHERE BRN ='&BRANCH' AND ACC = '&ACCOUNT'))
/
SELECT * FROM XAFNFC.ICTMS_RULE_SDE WHERE RULE_ID IN (SELECT RULE FROM XAFNFC.ICTMS_PR_INT
WHERE PRODUCT_CODE IN (SELECT DISTINCT PROD FROM XAFNFC.ICVWS_ACC_PR WHERE BRN ='&BRANCH' AND ACC = '&ACCOUNT'))
/
SELECT * FROM XAFNFC.ICTMS_RULE_UDE WHERE RULE_ID IN (SELECT RULE FROM XAFNFC.ICTMS_PR_INT
WHERE PRODUCT_CODE IN (SELECT DISTINCT PROD FROM XAFNFC.ICVWS_ACC_PR WHERE BRN ='&BRANCH' AND ACC = '&ACCOUNT'))
/
SELECT * FROM XAFNFC.ICTM_EXPR WHERE RULE_ID IN (SELECT RULE FROM XAFNFC.ICTMS_PR_INT
WHERE PRODUCT_CODE IN (SELECT DISTINCT PROD FROM XAFNFC.ICVWS_ACC_PR WHERE BRN ='&BRANCH' AND ACC = '&ACCOUNT'))
/
SELECT * FROM XAFNFC.ICTMS_IS_FMT_DESC WHERE  RULE_ID IN (SELECT RULE FROM XAFNFC.ICTMS_PR_INT
WHERE PRODUCT_CODE IN (SELECT DISTINCT PROD FROM XAFNFC.ICVWS_ACC_PR WHERE BRN ='&BRANCH' AND ACC = '&ACCOUNT'))
/
SELECT * FROM XAFNFC.ICTMS_PR_INT WHERE RULE IN (SELECT RULE FROM XAFNFC.ICTMS_PR_INT
WHERE PRODUCT_CODE IN (SELECT DISTINCT PROD FROM XAFNFC.ICVWS_ACC_PR WHERE BRN ='&BRANCH' AND ACC = '&ACCOUNT'))
/
PROMPT HOLIDAY
SELECT * FROM XAFNFC.STTM_LCL_HOLIDAY WHERE BRANCH_CODE='&BRANCH' AND YEAR = ( SELECT TO_CHAR(TODAY,'YYYY')
FROM XAFNFC.STTM_DATES WHERE BRANCH_CODE='&BRANCH');
SELECT * FROM XAFNFC.STTM_TRN_CODE WHERE TRN_CODE IN
(SELECT   TRANSACTION_CODE FROM XAFNFC.CSTMS_PRODUCT_EVENT_ACCT_ENTRY
WHERE PRODUCT_CODE IN (SELECT DISTINCT PROD FROM XAFNFC.ICVWS_ACC_PR WHERE BRN ='&BRANCH' AND ACC = '&ACCOUNT'))
/
SELECT * FROM XAFNFC.ICTBS_ITM_TOV WHERE ACC ='&ACCOUNT';
SELECT * FROM XAFNFC.ICVW_LINE_DET  WHERE ACC ='&ACCOUNT'
/
SELECT * FROM XAFNFC.ICTMS_RATE_DEF
WHERE RATE_CODE IN
 (
  SELECT RATE_CODE FROM XAFNFC.ICTBS_UDEVALS
   WHERE COND_KEY LIKE '%'||(SELECT ACCOUNT_CLASS||CCY FROM XAFNFC.STTM_CUST_ACCOUNT
                                                 WHERE CUST_AC_NO LIKE  '&ACCOUNT') ||'%'
   AND RATE_CODE IS NOT NULL
  UNION
  SELECT RATE_CODE FROM XAFNFC.ICTBS_UDEVALS
   WHERE COND_KEY LIKE '%&ACCOUNT%'
   AND RATE_CODE IS NOT NULL
  UNION
  SELECT RATE_CODE FROM XAFNFC.ICTMS_ACC_UDEVALS
   WHERE ACC = '&ACCOUNT'
   AND RATE_CODE IS NOT NULL
  UNION
  SELECT RATE_CODE FROM XAFNFC.ICTBS_UDEVALS_HISTORY
   WHERE COND_KEY LIKE '%&ACCOUNT%'
   AND RATE_CODE IS NOT NULL
 )
/
SELECT * FROM XAFNFC.ICTMS_RATES WHERE RATE_CODE IN
 (
  SELECT RATE_CODE FROM XAFNFC.ICTBS_UDEVALS
   WHERE COND_KEY LIKE '%'||(SELECT ACCOUNT_CLASS||CCY FROM XAFNFC.STTM_CUST_ACCOUNT
                                                 WHERE CUST_AC_NO LIKE  '&ACCOUNT') ||'%'
   AND RATE_CODE IS NOT NULL
  UNION
  SELECT RATE_CODE FROM XAFNFC.ICTBS_UDEVALS
   WHERE COND_KEY LIKE '%&ACCOUNT%'
   AND RATE_CODE IS NOT NULL
  UNION
  SELECT RATE_CODE FROM XAFNFC.ICTMS_ACC_UDEVALS
   WHERE ACC = '&ACCOUNT'
   AND RATE_CODE IS NOT NULL
  UNION
  SELECT RATE_CODE FROM XAFNFC.ICTBS_UDEVALS_HISTORY
   WHERE COND_KEY LIKE '%&ACCOUNT%'
   AND RATE_CODE IS NOT NULL
 )
/
SELECT * FROM XAFNFC.STTM_ACCOUNT_CLASS
WHERE ACCOUNT_CLASS IN
(
SELECT ACLASS FROM XAFNFC.ICTBS_ACC_PR
WHERE ACC = '&ACCOUNT'
);
SELECT * FROM XAFNFC.ICTBS_DR_INT_ERR
WHERE ACC = '&ACCOUNT'
/
