/
PROMPT LMTBS_LIMIT_VDCHG_LOG
SELECT *
  FROM XAFNFC.LMTBS_LIMIT_VDCHG_LOG
 WHERE CUST_ID IN (SELECT CUS.CUSTOMER_NO
                     FROM XAFNFC.LMTMS_LIMITS LM, STTM_CUSTOMER CUS
                    WHERE INTEREST_CALC_ACC ='&ACCOUNT'
                      AND CUS.LIABILITY_NO = LIAB_ID)
/
PROMPT NEWLY ICTBS_TD_REDEMPTION
SELECT * FROM XAFNFC.ICTBS_TD_REDEMPTION  WHERE BRANCH_CODE = '&BRANCH' AND CUST_AC_NO='&ACCOUNT';

PROMPT ACTB_DAILY_LOG FULL
SELECT * FROM XAFNFC.ACTB_DAILY_LOG WHERE AC_NO='&ACCOUNT' OR RELATED_ACCOUNT='&ACCOUNT';
PROMPT ICTB_TD_LOG
SELECT * FROM XAFNFC.ICTB_TD_LOG WHERE ACC='&ACCOUNT';
PROMPT MSTB_DLY_MSG_OUT
SELECT * FROM XAFNFC.MSTB_DLY_MSG_OUT WHERE CUST_AC_NO = '&ACCOUNT';
