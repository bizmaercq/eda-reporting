115    PROCEDURE dbg(p_l VARCHAR2) IS
116    BEGIN
117  	 debug.pr_debug('CL', 'REBUILD_COMP_BALANCES->' || p_l);
118    END dbg;
119  
120  BEGIN
121  
122    l_account := '024S12S122220002';
123    l_brn	 := '024';
124  	global.pr_init(l_brn,'SYSTEM');
125    dbg('account is == > ' || l_account);
126    l_rec_account := clpkss_cache.fn_account_master(l_account, l_brn);
127  
128    BEGIN
129  	 SELECT MIN(SCHEDULE_ST_DATE)
130  	   INTO l_mainint_date
131  	   FROM CLTB_ACCOUNT_SCHEDULES
132  	  WHERE ACCOUNT_NUMBER = l_account
133  	    AND BRANCH_CODE = l_brn
134  	    AND COMPONENT_NAME = 'MAIN_INT'
135  	    AND NVL(AMOUNT_DUE, 0) <> NVL(AMOUNT_SETTLED, 0)
136  	    AND NVL(AMOUNT_SETTLED, 0) = 0;
137    EXCEPTION
138  	 WHEN NO_DATA_FOUND THEN
139  
140  	   dbg('inside No data found while selecting main_int date');
141  	   SELECT MIN(SCHEDULE_ST_DATE)
142  	     INTO l_mainint_date
143  	     FROM CLTB_ACCOUNT_SCHEDULES
144  	    WHERE ACCOUNT_NUMBER = l_account
145  	      AND BRANCH_CODE = l_brn
146  	      AND COMPONENT_NAME = 'MAIN_INT'
147  	      AND NVL(AMOUNT_DUE, 0) <> NVL(AMOUNT_SETTLED, 0);
148    END;
149    dbg('Main int date ==>' || l_mainint_date);
150    BEGIN
151  	 SELECT MIN(SCHEDULE_ST_DATE)
152  	   INTO l_principal_date
153  	   FROM CLTB_ACCOUNT_SCHEDULES
154  	  WHERE ACCOUNT_NUMBER = l_account
155  	    AND BRANCH_CODE = l_brn
156  	    AND COMPONENT_NAME = 'PRINCIPAL'
157  	    AND NVL(AMOUNT_DUE, 0) <> NVL(AMOUNT_SETTLED, 0)
158  	    AND NVL(AMOUNT_SETTLED, 0) = 0;
159    EXCEPTION
160  	 WHEN NO_DATA_FOUND THEN
161  
162  	   dbg('inside No data found while selecting principal date');
163  	   SELECT MIN(SCHEDULE_ST_DATE)
164  	     INTO l_principal_date
165  	     FROM CLTB_ACCOUNT_SCHEDULES
166  	    WHERE ACCOUNT_NUMBER = l_account
167  	      AND BRANCH_CODE = l_brn
168  	      AND COMPONENT_NAME = 'PRINCIPAL'
169  	      AND NVL(AMOUNT_DUE, 0) <> NVL(AMOUNT_SETTLED, 0);
170    END;
171    dbg('principal date ==>' || l_principal_date);
172  
173    l_recalc_st_date := greatest(l_mainint_date, l_principal_date);
174  
175    dbg('Recalc start date ==> ' || l_recalc_st_date);
176  
177    IF C_PRIN_MOVEMENTS_NONAMRT%ISOPEN THEN
178  	 CLOSE C_PRIN_MOVEMENTS_NONAMRT;
179    END IF;
180  
181    IF C_PRIN_MOVEMENTS_AMRT%ISOPEN THEN
182  	 CLOSE C_PRIN_MOVEMENTS_AMRT;
183    END IF;
184    dbg('l_rec_account.amortized ==> ' || l_rec_account.amortized);
185  
186    IF l_rec_account.amortized is null or
187  	  l_rec_account.amortized not in ('N', 'Y') THEN
188  
189  	 BEGIN
190  
191  	   SELECT 1
192  	     INTO l_amort_count
193  	     FROM cltbs_account_comp_sch
194  	    WHERE account_number = l_account
195  	      AND branch_code = l_brn
196  	      AND component_name = 'PRINCIPAL'
197  	      AND schedule_type = 'P'
198  	      AND ROWNUM < 2;
199  	 EXCEPTION
200  	   WHEN NO_DATA_FOUND THEN
201  	     dbg('In no data found' || l_amort_count);
202  	     l_amort_count := 0;
203  	 END;
204  	 dbg('GET AMORT COUNT HERE ==>' || l_amort_count);
205  	 IF l_amort_count <> 0 then
206  	   l_rec_account.amortized := 'N';
207  	 ELSE
208  	   l_rec_account.amortized := 'Y';
209  	 END IF;
210    END IF;
211  
212    IF l_rec_account.amortized = 'N' THEN
213  	 OPEN C_PRIN_MOVEMENTS_NONAMRT(l_account, l_brn);
214  	 FETCH C_PRIN_MOVEMENTS_NONAMRT BULK COLLECT
215  	   INTO l_tb_movements_in_period;
216  	 CLOSE C_PRIN_MOVEMENTS_NONAMRT;
217  
218    ELSIF l_rec_account.amortized = 'Y' THEN
219  	 OPEN C_PRIN_MOVEMENTS_AMRT(l_account, l_brn);
220  	 FETCH C_PRIN_MOVEMENTS_AMRT BULK COLLECT
221  	   INTO l_tb_movements_in_period;
222  	 CLOSE C_PRIN_MOVEMENTS_AMRT;
223  
224    END IF;
225  
226    dbg('l_tb_movements_count' || l_tb_movements_in_period.COUNT);
227    IF l_tb_movements_in_period.COUNT > 0 THEN
228  
229  	 FOR i IN l_tb_movements_in_period.FIRST .. l_tb_movements_in_period.LAST LOOP
230  
231  	   dbg('PAY_RECV ==>' || l_tb_movements_in_period(i).PAY_RECV);
232  	   dbg('PAID_DATE==>' || l_tb_movements_in_period(i).paid_date);
233  	   dbg('AMT_PAID ==>' || l_tb_movements_in_period(i).Amt_Paid);
234  	   dbg('BALANCE ==> ' || l_balance);
235  
236  	   IF l_tb_movements_in_period(i).PAY_RECV = 'D' THEN
237  	     l_balance := l_balance + l_tb_movements_in_period(i).Amt_Paid;
238  	   ELSE
239  	     l_balance := l_balance - l_tb_movements_in_period(i).Amt_Paid;
240  	   END IF;
241  
242  	   dbg('CHECK BALANCE HERE ==> ' || l_balance);
243  	   IF l_balance < 0 THEN
244  	     l_balance := 0;
245  	   END IF;
246  
247  	   BEGIN
248  	     INSERT INTO CLTB_ACCOUNT_COMP_BALANCES
249  	       (branch_code, account_number, component_name, val_date, balance)
250  	     VALUES
251  	       (l_brn,
252  		l_account,
253  		'PRINCIPAL_EXPECTED',
254  		l_tb_movements_in_period(i).paid_date,
255  		l_balance);
256  	   EXCEPTION
257  	     WHEN DUP_VAL_ON_INDEX THEN
258  	       UPDATE CLTB_ACCOUNT_COMP_BALANCES
259  		  SET BALANCE = l_balance
260  		WHERE ACCOUNT_NUMBER = l_account
261  		  AND branch_code = l_brn
262  		  AND component_name = 'PRINCIPAL_EXPECTED'
263  		  AND VAL_DATE = l_tb_movements_in_period(i).paid_date;
264  
265  	   END;
266  	 END LOOP;
267  
268  	 dbg('Comp_balances has been rebuilt now call recalc');
269  	 dbg('now print comp bal');
270  	 FOR j IN (select *
271  		     from cltb_account_comp_balances
272  		    where account_number = l_account
273  		      and branch_code = l_brn
274  		      and component_name = 'PRINCIPAL_EXPECTED') LOOP
275  	   dbg(' val_date :: ' || j.val_date || ' balance :: ' || j.balance);
276  	 END LOOP;
277  
278  	 update cltb_account_master
279  	    set calc_reqd = 'Y', recalc_action_code = 'E'
280  	  where branch_code = l_brn
281  	    AND ACCOUNT_NUMBER = l_account;
282  
283  	 dbg('now calling fn_recalc_for_an_account ' || l_recalc_st_date);
284  	 if not clpkss_recalc.fn_recalc_for_an_account(l_account,
285  						       l_brn,
286  						       l_recalc_st_date,
287  						       p_err_code,
288  						       p_err_param) THEN
289  
290  	   dbg('failure ' || p_err_code || '::' || p_err_param || '::' ||
291  	       SQLERRM);
292  	   rollback;
293  	 ELSE
294  	   dbg('success');
295  	 END IF;
296    END IF;
297  EXCEPTION
298    WHEN OTHERS THEN
299  	 dbg('In when others' || SQLERRM);
300  	 p_err_param := SUBSTR(SQLERRM,1,80);
301  	 rollback;
302  
303  END;
