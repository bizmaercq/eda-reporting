 14    CURSOR C_PRIN_MOVEMENTS_NONAMRT(l_account cltb_account_apps_master.account_number%type, l_brn cltb_account_apps_master.branch_code%type) IS
 15  	 SELECT Paid_Date,
 16  		Nvl(SUM(Nvl(Amount_Paid, 0) + Nvl(Amount_Waived, 0)), 0) Amt_Paid,
 17  		'P' PAY_RECV
 18  	   FROM Cltb_Amount_Paid
 19  	  WHERE Branch_Code = l_brn
 20  	    AND Account_Number = l_account
 21  	    AND Component_Name = 'PRINCIPAL'
 22  	    AND PAID_DATE >= l_rec_account.VALUE_DATE
 23  	    and paid_status = 'P'
 24  	    AND DUE_DATE > PAID_DATE
 25  	  GROUP BY Paid_Date
 26  	 UNION ALL
 27  	 SELECT RECD_DATE PAID_DATE,
 28  		NVL(SUM(AMOUNT_RECD), 0) AMT_PAID,
 29  		'D' PAY_RECV
 30  	   FROM CLTB_AMOUNT_RECD
 31  	  WHERE Branch_Code = l_brn
 32  	    AND Account_Number = l_account
 33  	    AND Component_Name = 'PRINCIPAL'
 34  	    AND recd_date >= l_rec_account.VALUE_DATE
 35  	  GROUP BY RECD_DATE
 36  	 UNION ALL
 37  	 SELECT SCHEDULE_DUE_DATE PAID_DATE,
 38  		(NVL(SUM(AMOUNT_DUE), 0) -
 39  		(SELECT NVL(SUM(Nvl(Amount_Paid, 0)), 0)
 40  		    FROM CLTB_AMOUNT_PAID B
 41  		   WHERE Account_Number = l_account
 42  		     AND Branch_Code = l_brn
 43  		     AND Component_Name = 'PRINCIPAL'
 44  		     AND DUE_DATE = SCHEDULE_DUE_DATE
 45  		     AND PAID_DATE >= l_rec_account.VALUE_DATE
 46  		     AND PAID_DATE < DUE_DATE
 47  		     AND paid_status = 'P')) AMT_PAID,
 48  		'S' PAY_RECV
 49  	   FROM CLTB_ACCOUNT_SCHEDULES A
 50  	  WHERE Branch_Code = l_brn
 51  	    AND Account_Number = l_account
 52  	    AND Component_Name = 'PRINCIPAL'
 53  	  GROUP BY SCHEDULE_DUE_DATE
 54  	 UNION ALL
