CREATE TABLESPACE fcc_DATA_SMALL DATAFILE 
  'D:\ORACLE\PRODUCT\10.1.0\ORADATA\FCCTEST1\fcc_DATA_SMALL.DBF' SIZE 2048M AUTOEXTEND ON NEXT 5M MAXSIZE UNLIMITED
LOGGING
ONLINE
PERMANENT
EXTENT MANAGEMENT LOCAL UNIFORM SIZE 1M
BLOCKSIZE 8K
SEGMENT SPACE MANAGEMENT AUTO
FLASHBACK ON;

