SELECT c.local_branch,c.customer_no as IdInterneClt,c.customer_name1,null as nom_naissance,null as prenom,null as Nom_marital,null as num_piece,null as date_delivrance,null as lieu_delivrance,null as date_validite,null as Nom_Du_Pere,null as Prenom_pere,null as nom_naissance_mere,null as prenom_mere from xafnfc.sttm_customer c , sttm_cust_personal p where p.customer_no = c.customer_no and  c.customer_type ='I' and c.record_stat ='O' and p.ppt_iss_date is  null and c.customer_type ='I' and c.record_stat ='O' and c.local_branch = '020' order by c.customer_name1;
